{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "max-len": [1, 70, 2, { "ignoreComments": true }],
    "react/prop-types": [2],
    "linebreak-style": 0
  },
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["airbnb-base", "plugin:react/recommended"]
}